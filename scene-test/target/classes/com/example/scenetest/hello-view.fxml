<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox prefHeight="604.0" prefWidth="650.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.scenetest.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Button fx:id="btnOpen" mnemonicParsing="false" onAction="#btnPressOpen" text="Open">
         <VBox.margin>
            <Insets />
         </VBox.margin></Button>
      <Button fx:id="btnSave" mnemonicParsing="false" onAction="#btnPressSave" text="Save">
         <VBox.margin>
            <Insets />
         </VBox.margin></Button>
      <HBox alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="btnRandNum" mnemonicParsing="false" onAction="#btnPressRandNum" text="Random Num." />
         </children>
      </HBox>
      <TextField prefHeight="25.0" prefWidth="340.0">
         <VBox.margin>
            <Insets />
         </VBox.margin></TextField>
      <TextArea fx:id="fldTxtArea" prefHeight="200.0" prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="-200.0" />
         </VBox.margin>
      </TextArea>
   
       <Label fx:id="welcomeText" />
   </children>
</VBox>
